<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns="http://javafx.com/javafx/8.0.112"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="displays.LegendDisplay"
      alignment="CENTER" spacing="10.0">
    <padding>
        <Insets bottom="5.0" top="10.0"/>
    </padding>
    <Label text="No Match">
        <graphic>
            <Canvas height="10.0" width="10.0" fx:id="noMatchColorCanvas" />
        </graphic>
    </Label>
    <Label text="Match">
        <graphic>
            <Canvas height="10.0" width="10.0" fx:id="matchColorCanvas" />
        </graphic>
    </Label>
    <Label text="Match Start">
        <graphic>
            <Canvas height="10.0" width="10.0" fx:id="matchStartColorCanvas"/>
        </graphic>
    </Label>
</HBox>
