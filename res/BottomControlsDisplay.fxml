<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns="http://javafx.com/javafx/8.0.112"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="displays.BottomControlsDisplay"
      alignment="CENTER" spacing="10.0">
    <padding>
        <Insets bottom="10.0" top="5.0"/>
    </padding>
    <Label contentDisplay="RIGHT" text="Matched words:">
        <graphic>
            <ComboBox prefWidth="150.0" fx:id="matchesComboBox" onAction="#matchSelectedHandler"/>
        </graphic>
    </Label>
</HBox>
